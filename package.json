{
  "name": "labs-api-starter",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "repository": "github:Lambda-School-Labs/labs-api-starter",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "format": "npx prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md)\"",
    "knex": "npx knex --knexfile config/knexfile.js",
    "tests": "npx jest",
    "coverage": "CI=true npx jest --coverage --detectOpenHandles --forceExit",
    "up": "npx knex --knexfile config/knexfile.js migrate:up",
    "down": "npx knex --knexfile config/knexfile.js migrate:down",
    "seed": "npx knex --knexfile config/knexfile.js seed:run",
    "update": "npx knex --knexfile config/knexfile.js migrate:rollback && npx knex --knexfile config/knexfile.js migrate:latest && npx knex --knexfile config/knexfile.js seed:run",
    "test": "cross-env NODE_ENV=testing jest --watch --verbose --runInBand --silent"
  },
  "lint-staged": {
    "api/**/*.js": [
      "eslint --fix",
      "prettier-eslint --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "testResultsProcessor": "jest-sonar-reporter",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "/vendor/",
      "/coverage/"
    ],
    "collectCoverageFrom": [
      "**/*.js"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run start",
    "watch": [
      "api/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.6",
    "aws-sdk": "^2.1354.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cacache": "^15.0.5",
    "connect-session-knex": "^2.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "csv-string": "^4.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.6.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.2",
    "faker": "^4.1.0",
    "helmet": "^3.23.3",
    "http-errors": "~1.6.3",
    "json2csv": "^5.0.5",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.5.0",
    "knex": "^0.21.19",
    "knex-cleaner": "^1.3.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.6.3",
    "pdf": "^0.1.0",
    "pg": "^8.5.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "request": "^2.88.2",
    "sqlite3": "^5.1.5",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.0",
    "jest": "^25.2.4",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^10.4.2",
    "nodemon": "^2.0.12",
    "prettier": "2.3.1",
    "supertest": "^4.0.2"
  }
}
